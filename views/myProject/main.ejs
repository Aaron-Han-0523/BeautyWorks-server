<!DOCTYPE html>
<html lang="ko">

<head>
  <%- include ("../partials/header.ejs") %>
    <!-- 이미지 그룹 슬라이드 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.5.1/css/swiper.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.5.1/js/swiper.js"></script>
</head>

<body class="layout-boxed">

  <%- include ("../partials/loader.ejs") %>

    <%- include ("../partials/client-nav.ejs") %>
      <%- include ("../partials/chatMark.ejs") %>

        <!--  BEGIN MAIN CONTAINER  -->
        <div class="main-container" id="container">

          <div class="overlay"></div>
          <div class="search-overlay"></div>

          <%- include ("../partials/client-sidebar.ejs") %>

            <!--  BEGIN CONTENT AREA  -->
            <div id="content" class="main-content">
              <div class="layout-px-spacing">

                <div class="middle-content container-xxl p-0">

                  <!-- BREADCRUMB -->
                  <div class="row px-3">
                    <div class="breadcrumb align-items-end d-flex pt-3 br-20">
                      <div class="p-1 col-3">
                        <h5><strong>
                            <%= __('MY PROJECT') %>
                          </strong></h5>
                      </div>
                    </div>
                  </div>
                  <!-- CONTENT AREA -->
                  <div class="row layout-top-spacing">

                    <ul>
                      <!-- 임시 저장한 Progress -->
                      <div>
                        <hr>
                        <div class="ms-5">
                          <li>
                            <h5><strong>
                                <%= __('users.dashboard.header.TempProgress') %>
                              </strong></h5>
                          </li>
                        </div>
                        <hr>

                        <div class="">
                          <div class="mt-2 ms-4 d-flex align-items-center">
                            <h6>My Suncream</h6>
                            <div class="bg-gray ms-3 p-2 h6">2022-11-03</div>
                            <% let test_temp_project={detailPhase:0} %>
                          </div>
                          <div class="row mx-0 mt-2">
                            <div class="col progress-container">
                              <% const temp_project_phase=0;%>
                                <% const n_phase0=codezip.numberOfProjectState.detailPhase[0];%>
                                  <% let progress_width=((test_temp_project.detailPhase+1)/n_phase0)*100+'%';%>
                                    <div class="progress" style="width: <%= progress_width %>;"> </div>
                                    <% for(let i=0;i< n_phase0;i++){ %>
                                      <div class="text-break">
                                        <%= __("project_phase"+temp_project_phase+'.'+i)%>
                                      </div>
                                      <% } %>
                            </div>
                            <div class="col-1"></div>
                          </div>
                        </div>

                      </div>



                      <!-- Progress -->
                      <div class="form-group ms-3">
                        <div class="row">
                          <div class="col-6 col-md-4">
                            <h5><strong>
                                <%= __('users.dashboard.header.Progress') %>
                              </strong></h5>
                          </div>
                        </div>
                        <div class="row mt-3">
                          <div class="col-12 col-md-12">
                            <h6>1. My Suncream</h6>
                            <%- include("../partials/progressbar/"+1+".ejs") %>
                          </div>
                        </div>
                        <div class="row">
                          <div class="col-12 col-md-12">
                            <h6>2. My Suncream</h6>
                            <%- include("../partials/progressbar/"+2+".ejs") %>
                          </div>
                        </div>
                        <div class="row">
                          <div class="col-12 col-md-12">
                            <h6>3. My Suncream</h6>
                            <%- include("../partials/progressbar/"+3+".ejs") %>
                          </div>
                        </div>

                        <div class="row text-end">
                          <a href="<%= codezip.url.users.myProject.main %>">
                            <i class="fa-solid fa-chevron-right"></i>
                          </a>
                        </div>

                      </div>


                      <!-- Completed Project -->
                      <div class="form-group mx-3">
                        <li class="row">
                          <div class="col-6 col-md-4">
                            <h5><strong>
                                <%= __('users.dashboard.header.CompletedProject') %>
                              </strong></h5>
                          </div>
                        </li>
                        <div class="row mt-3">
                          <div class="swiper-container">
                            <div class="swiper-wrapper">

                              <div class="swiper-slide">
                                <div>
                                  <img src="https://ktsmemo.cafe24.com/p/0740.jpg">
                                  <div class="row mt-3 justify-content-center">
                                    <p>헤라 셀 에센스<br />
                                      바이옴 플러스 150ml<br />
                                      Total 발주 수량 : 100000<br />
                                      최근 발주 날짜: 2022.10.04</p>
                                    <div><button class="col-12 btn" type="button">REORDER</button></div>
                                  </div>
                                </div>
                              </div>
                              <div class="swiper-slide">
                                <div>
                                  <img src="http://ktsmemo.cdn3.cafe24.com/p/0387.jpg">
                                  <div class="row mt-3 justify-content-center">
                                    <p>헤라 셀 에센스<br />
                                      바이옴 플러스 150ml<br />
                                      Total 발주 수량 : 100000<br />
                                      최근 발주 날짜: 2022.10.04</p>
                                    <div><button class="col-12 btn" type="button">REORDER</button></div>
                                  </div>
                                </div>
                              </div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0553.jpg"></div>
                              <div class="swiper-slide"><img src="http://ktsmemo.cdn3.cafe24.com/p/0455.jpg"></div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0099.jpg"></div>
                              <div class="swiper-slide"><img src="https://ktsmemo.cafe24.com/p/0716.jpg"></div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0786.jpg"></div>
                              <div class="swiper-slide"><img src="http://ktsmemo.cdn3.cafe24.com/p/0731.jpg"></div>
                              <div class="swiper-slide"><img src="http://ktsmemo.cdn3.cafe24.com/p/0742.jpg"></div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0698.jpg"></div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0095.jpg"></div>
                              <div class="swiper-slide"><img src="http://oldmidi.cdn3.cafe24.com/p/0315.jpg"></div>
                              <div class="swiper-slide"><img src="http://ktsmemo.cdn3.cafe24.com/p/0625.jpg"></div>
                              <div class="swiper-slide"><img src="https://ktsmemo.cafe24.com/p/0289.jpg"></div>
                              <div class="swiper-slide"><img src="https://ktsmemo.cafe24.com/p/0646.jpg"></div>
                              <div class="swiper-slide">- 끝 -</div>
                            </div>
                          </div>

                          <!-- 인디케이터 -->
                          <!-- <div class="swiper-indicator"></div> -->

                          <!-- 네비게이션 -->
                          <!-- 이전 버튼 -->
                          <div class="swiper-btn-prev"><i class="fa-solid fa-chevron-left"></i></div>
                          <!-- 다음 버튼 (오른쪽에 있는 버튼) -->
                          <div class="swiper-btn-next"><i class="fa-solid fa-chevron-right"></i></div>

                        </div>

                      </div>

                    </ul>

                  </div>
                </div>
                <!--  END CONTENT AREA  -->

                <%- include ("../partials/footer.ejs") %>

              </div>
              <!-- END MAIN CONTAINER -->

              <%- include ("../partials/commonScrpits.ejs") %>

                <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
                <script>
                  let _ = new Swiper('.swiper-container', {

                    slidesPerView: 4, // 동시에 보여줄 슬라이드 갯수
                    spaceBetween: 10, // 슬라이드간 간격
                    slidesPerGroup: 4, // 그룹으로 묶을 수, slidesPerView 와 같은 값을 지정하는게 좋음

                    // 그룹수가 맞지 않을 경우 빈칸으로 메우기
                    // 3개가 나와야 되는데 1개만 있다면 2개는 빈칸으로 채워서 3개를 만듬
                    loopFillGroupWithBlank: true,

                    loop: true, // 무한 반복

                    pagination: { // 페이징
                      el: '.swiper-indicator',
                      clickable: true, // 페이징을 클릭하면 해당 영역으로 이동, 필요시 지정해 줘야 기능 작동
                    },
                    navigation: { // 네비게이션
                      nextEl: '.swiper-btn-next', // 다음 버튼 클래스명
                      prevEl: '.swiper-btn-prev', // 이번 버튼 클래스명
                    }
                  });
                </script>
                <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
</body>

</html>