<!-- page:현재페이지 번호, end_page:끝페이지 번호, href:베이스URL, page_query:페이지 쿼리키 -->
<div class="d-flex justify-content-center mt-4">
  <!-- 페이징처리 시작 -->
  <!-- 처음 & 이전페이지 -->
  <ul class="p-2 pagination">
    <% if (page==1) { %>
    <li class="page-item disabled"><a class="page-link">
        <%- __('users.pagination.처음으로') %>
      </a>
    </li>
    <li class="page-item disabled"><a class="page-link">
        <%- __('users.pagination.이전으로') %>
      </a>
    </li>
    <% } else { %>
    <li class="page-item"><a class="page-link" href="<%= href %>?<%= page_query %>=1">
        <%- __('users.pagination.처음으로') %>
      </a>
    </li>
    <li class="page-item"><a class="page-link" href="<%= href %>?<%= page_query %>=<%= page-1 %>">
        <%- __('users.pagination.이전으로') %>
      </a>
    </li>
    <% } %>
  </ul>
  <!-- 페이지 리스트 -->
  <ul class="p-2 pagination">
    <% let end_list_num=end_page> 6 ? 5:end_page %>
    <% if (page < 4) { for (let i=1; i<=end_list_num; i++) { %>
    <% if (page==i) { %>
    <li class="page-item disabled">
      <a class="page-link current_page">
        <%= i %>
      </a>
    </li>
    <% } else { %>
    <li class="page-item"><a class="page-link" href="<%= href %>?<%= page_query %>=<%= i %>">
        <%= i %>
      </a></li>
    <% }} %>
    <% } else if ( page> end_page-3 ) { %>
    <% for (let i = end_page-4; i <= end_page; i++) { %>
    <% if (page==i) { %>
    <li class="page-item disabled">
      <a class="page-link current_page">
        <%= i %>
      </a>
    </li>
    <% } else { %>
    <li class="page-item">
      <a class="page-link" href="<%= href %>?<%= page_query %>=<%= i %>">
        <%= i %>
      </a>
    </li>
    <% }} %>
    <% } else { for (let i=page-2; i < page+3; i++) { if (page==i) {
                                    %>
    <li class="page-item disabled">
      <a class="page-link current_page">
        <%= i %>
      </a>
    </li>
    <% } else { %>
    <li class="page-item">
      <a class="page-link" href="<%= href %>?<%= page_query %>=<%= i %>">
        <%= i %>
      </a>
    </li>
    <% }} %>
    <% } %>
  </ul>
  <!-- 다음 & 마지막페이지 -->
  <ul class="p-2 pagination">
    <% if(page==end_page){ %>
    <li class="page-item disabled">
      <a class="page-link">
        <%- __('users.pagination.다음으로') %>
      </a>
    </li>
    <li class="page-item disabled">
      <a class="page-link">
        <%- __('users.pagination.마지막으로') %>
      </a>
    </li>
    <% } else { %>
    <li class="page-item">
      <a class="page-link" href="<%= href %>?<%= page_query %>=<%= page+1 %>">
        <%- __('users.pagination.다음으로') %>
      </a>
    </li>
    <li class="page-item">
      <a class="page-link" href="<%= href %>?<%= page_query %>=<%= end_page %>">
        <%- __('users.pagination.마지막으로') %>
      </a>
    </li>
    <% } %>
  </ul>
  <!-- 페이징처리 끝 -->
</div>